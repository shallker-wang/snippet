<div class="row">
  <div class="span12 accordion">
    <button class="btn" id="a-back"><i class="icon-arrow-left"></i> Back</button>
    <button class="btn pull-right" id="a-update"><i class="icon-check"></i> Update</button>
  </div>
</div>
<div class="row">
    <div class="span6">
        <textarea rows="2" id="snippet-doc">{{=it.doc || ''}}</textarea>
    </div>
    <div class="span6">
        <div class="editor-read" id="ace-editor">{{=it.code}}</div>
    </div>
</div>
